    "homepage": "https://github.com/Denobraz/yii2-image-manager",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Denobraz",
			"homepage": "https://github.com/Denobraz" 
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
		"noam148/yii2-image-resize" : "dev-master",
		"kartik-v/yii2-widget-fileinput": "@dev"
    },
    "autoload": {
